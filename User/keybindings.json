[
  {
    "key": "cmd+r",
    "command": "workbench.action.openRecent"
  },
  {
    "key": "shift+cmd+=",
    "command": "search.action.focusNextSearchResult",
    "when": "hasSearchResult"
  },
  {
    "key": "f4",
    "command": "-search.action.focusNextSearchResult",
    "when": "hasSearchResult"
  },
  {
    "key": "shift+cmd+-",
    "command": "search.action.focusPreviousSearchResult",
    "when": "hasSearchResult"
  },
  {
    "key": "shift+f4",
    "command": "-search.action.focusPreviousSearchResult",
    "when": "hasSearchResult"
  },
  {
    "key": "cmd+9",
    "command": "workbench.action.terminal.focus"
  },
  {
    "key": "alt+z",
    "command": "workbench.action.toggleZenMode"
  },
  {
    "key": "cmd+k z",
    "command": "-workbench.action.toggleZenMode"
  },
  {
    "key": "alt+o",
    "command": "outline.focus"
  },
  {
    "key": "ctrl+h",
    "command": "-deleteLeft",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "ctrl+h",
    "command": "-extension.vim_ctrl+h",
    "when": "editorTextFocus && vim.active && vim.use<C-h> && !inDebugRepl"
  },
  {
    "key": "cmd+i",
    "command": "editor.action.triggerSuggest",
    "when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly"
  },
  {
    "key": "ctrl+space",
    "command": "-editor.action.triggerSuggest",
    "when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly"
  },
  {
    "key": "cmd+i",
    "command": "-expandLineSelection",
    "when": "textInputFocus"
  },
  {
    "key": "cmd+]",
    "command": "-editor.action.indentLines",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "cmd+[",
    "command": "-editor.action.outdentLines",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "cmd+[",
    "command": "editor.fold",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+cmd+[",
    "command": "-editor.fold",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+cmd+]",
    "command": "-editor.unfold",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+cmd+[",
    "command": "editor.foldRecursively",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+k cmd+[",
    "command": "-editor.foldRecursively",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+cmd+[",
    "command": "-workbench.action.previousEditor"
  },
  {
    "key": "shift+cmd+]",
    "command": "editor.unfoldRecursively",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+k cmd+]",
    "command": "-editor.unfoldRecursively",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+cmd+]",
    "command": "-workbench.action.nextEditor"
  },
  {
    "key": "ctrl+shift+cmd+k",
    "command": "editsHistory.moveCursorToPreviousEdit"
  },
  {
    "key": "cmd+j",
    "command": "-editsHistory.moveCursorToPreviousEdit"
  },
  {
    "key": "shift+cmd+j",
    "command": "-editsHistory.moveCursorToPreviouslyEditedFile"
  },
  {
    "key": "ctrl+cmd+j",
    "command": "editsHistory.moveCursorToNextEditedFile"
  },
  {
    "key": "shift+cmd+k",
    "command": "-editsHistory.moveCursorToNextEditedFile"
  },
  {
    "key": "ctrl+shift+cmd+j",
    "command": "editsHistory.moveCursorToNextEdit"
  },
  {
    "key": "cmd+k",
    "command": "-editsHistory.moveCursorToNextEdit"
  },
  {
    "key": "ctrl+shift+j",
    "command": "editsHistory.moveCursorToNextEditInSameFile"
  },
  {
    "key": "shift+cmd+i",
    "command": "-editsHistory.moveCursorToNextEditInSameFile"
  },
  {
    "key": "ctrl+shift+k",
    "command": "editsHistory.moveCursorToPreviousEditInSameFile"
  },
  {
    "key": "shift+cmd+u",
    "command": "-editsHistory.moveCursorToPreviousEditInSameFile"
  },
  {
    "key": "ctrl+cmd+k",
    "command": "editsHistory.moveCursorToPreviouslyEditedFile"
  },
  {
    "key": "cmd+]",
    "command": "editor.unfold",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "key": "alt+cmd+]",
    "command": "-editor.unfold",
    "when": "editorTextFocus && foldingEnabled"
  }
]
